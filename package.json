{
  "name": "young-votes-count",
  "version": "1.0.0",
  "description": "A web app that lets young people cast mock votes using Facebook",
  "main": "src/app/index.jsx",
  "scripts": {
    "lint": "eslint src/",
    "mocha": "mocha spec/**/*.spec.* --recursive --require babel-register --require spec-setup.js",
